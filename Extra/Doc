--New UI
Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Anti-VenomRBX/Anti-Venom/main/Lib"))({
    cheatname = 'Anti',
    gamename = 'Game',
    fileext = '.txt'
})

flags = Library.flags
Options = Library.options
Utility = Library.utility
Library:init()



--New window
local Utility, Window, Tabs, Sections, Settings = Library.utility, nil, {}, {}, nil; do
    -- Put your tabs and sections in here
    Window = Library.NewWindow({title = ('Anti-Venom'), size = UDim2.new(0, 500, 0, 600)})
    --New Tab
    Combat = Window:AddTab("Combat")
    
    --Settings tab (needed)
    Settings = Library:CreateSettingsTab(Window)
    
    --New Section (1 left 2 right)
    Aimbotsection = Combat:AddSection("Aimbot", 1)

end

--New Button
Aimbotsection:AddButton({text = "Button", risky = false, callback = function()

end});

--New Toggle
Aimbotsection:AddToggle({text = "Toggle", tooltip = "", flag = "", risky = false, callback = function(state)

end})

--New Slider
Aimbotsection:AddSlider({text = "Slider", flag = "", enabled = true, suffix = "", min = 1, max = 5000, value = 1000, risky = false, increment = 1, callback = function(Value)

end})

--New Dropdown
Aimbotsection:AddList({text = "Dropdown", flag = "", tooltip = "", enabled = true, selected = "", values = {"", ""}, risky = false, callback = function(Position)

end})

--New KeyBind
Aimbotsection:AddBind({text = "KeyBind", flag = "", tooltip = "", mode = "toggle", risky = false, callback = function(State)

end});

--New TextBox
Aimbotsection:AddBox({text = "TextBox", flag = "", enabled = true, input = "", risky = false, callback = function(txt)

end});

--New ColorPicker
Aimbotsection:AddColor({text = "ColorPicker", flag = "", enabled = true, color = Color3.fromRGB(40, 252, 3), risky = false, callback = function(Color)

end});
